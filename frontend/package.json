    "babel-preset-es2015": "^6.13.2",
    "chai": "^3.5.0",
    "chimp": "^0.40.4",
    "css-loader": "^0.23.1",
    "eslint": "^3.3.0",
    "eslint-config-standard": "^5.3.5",
    "eslint-plugin-html": "^1.5.2",
    "eslint-plugin-promise": "^2.0.1",
    "eslint-plugin-react": "^6.0.0",
    "eslint-plugin-standard": "^2.0.0",
    "html-webpack-plugin": "^2.22.0",
